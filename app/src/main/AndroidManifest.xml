<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cibus.com.cibus">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="Cibus"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GerenciarPosto"
            android:label="GerenciarPosto">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".InserirPostos"
            android:label="InserirPostos">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".GerenciarPosto" />
        </activity>
        <activity android:name=".consultaPostos"
                  android:label="Postos">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
    </application>

</manifest>